            </div>
            <div class="form-group">
                <label asp-for="Ative" class="control-label"></label>
                <input asp-for="Ative" class="form-control" />
                <span asp-validation-for="Ative" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AtivationCode" class="control-label"></label>
                <input asp-for="AtivationCode" class="form-control" />
                <span asp-validation-for="AtivationCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Password" class="control-label"></label>
                <input asp-for="Password" class="form-control" />
                <span asp-validation-for="Password" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TempPassword" class="control-label"></label>
                <input asp-for="TempPassword" class="form-control" />
                <span asp-validation-for="TempPassword" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IpComputerLast" class="control-label"></label>
                <input asp-for="IpComputerLast" class="form-control" />
                <span asp-validation-for="IpComputerLast" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Family" class="control-label"></label>
                <input asp-for="Family" class="form-control" />
                <span asp-validation-for="Family" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Tel" class="control-label"></label>
                <input asp-for="Tel" class="form-control" />
                <span asp-validation-for="Tel" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CityName" class="control-label"></label>
                <input asp-for="CityName" class="form-control" />
                <span asp-validation-for="CityName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Address" class="control-label"></label>
                <input asp-for="Address" class="form-control" />
                <span asp-validation-for="Address" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FkProvince" class="control-label"></label>
                <input asp-for="FkProvince" class="form-control" />
                <span asp-validation-for="FkProvince" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IpComputer" class="control-label"></label>
                <input asp-for="IpComputer" class="form-control" />
                <span asp-validation-for="IpComputer" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">برگشت به لیست</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
